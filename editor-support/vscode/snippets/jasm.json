{
    "System Write Call": {
        "prefix": "write",
        "body": [
            "move rax, 1       # sys_write",
            "move rdi, 1       # stdout file descriptor",
            "move rsi, $1      # buffer address",
            "move rdx, $2      # length",
            "call"
        ],
        "description": "Write data to stdout using sys_write"
    },
    "System Read Call": {
        "prefix": "read",
        "body": [
            "move rax, 0       # sys_read",
            "move rdi, 0       # stdin file descriptor",
            "move rsi, $1      # buffer address",
            "move rdx, $2      # buffer size",
            "call",
            "move [$3], rax    # save bytes read"
        ],
        "description": "Read data from stdin using sys_read"
    },
    "System Exit Call": {
        "prefix": "exit",
        "body": [
            "move rax, 60      # sys_exit",
            "move rdi, $1      # exit status",
            "call"
        ],
        "description": "Exit program with status code"
    },
    "String Data": {
        "prefix": "dstr",
        "body": "data $1 \"$2\"",
        "description": "Define string data"
    },
    "Buffer Data": {
        "prefix": "dbuf",
        "body": "data $1 size $2",
        "description": "Define buffer with size"
    },
    "Loop Structure": {
        "prefix": "loop",
        "body": [
            "$1:",
            "    $2",
            "    move rax, [$3]    # load counter",
            "    move rbx, $4      # compare value",
            "    comp rax, rbx",
            "    jumplt $1         # loop while less than"
        ],
        "description": "Create a basic loop structure"
    }
}